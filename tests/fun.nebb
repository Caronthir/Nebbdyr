# Non-recursive
# fibonacci
fun fib(limit):
    mut var a := 0
    mut var b := 1

    while a < limit:
        print a
        var temp := a
        a := b
        b := temp + b

fun show(x):
    print x

# Recursive fibonacci
fun fibonacci(n):
    if n <= 1:
        return n
    return fibonacci(n-2) + fibonacci(n-1)

print fibonacci(10)

